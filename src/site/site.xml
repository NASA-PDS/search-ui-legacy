  <version/>

  <body>
    <links>
      <item name="Search UI" href="https://pds-cm.jpl.nasa.gov/pds4/search/search-ui/index.html" />
    </links>

    <menu name="Software Documentation">
      <item name="About Search UI" href="index.html"/>
      <item name="Installation" href="install/index.html"/>
      <item name="Operation" href="operate/index.html"/>
    </menu>

    <menu name="Download">
      <item name="Binary Package (tar.gz)" href="https://pds.nasa.gov/software/pds4/search/${project.artifactId}-${project.version}-bin.tar.gz"/>
      <item name="Binary Package (zip)" href="https://pds.nasa.gov/software/pds4/search/${project.artifactId}-${project.version}-bin.zip"/>
    </menu>

    <menu ref="reports"/>
